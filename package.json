{
  "name": "relaxjs",
  "version": "0.3.0",
  "description": "A library written in Typescript to build RESTful services in node.js",
  "author": {
    "name": "Michele Ursino",
    "email": "michele@micurs.com",
    "url": "http://github.com/micurs"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/micurs/relaxjs"
  },
  "license": "MIT",
  "main": "dist/relaxjs.js",
  "bugs": "https://github.com/micurs/relaxjs/issues",
  "keywords": [
    "REST",
    "RESTful",
    "webservices",
    "typescript",
    "no routing",
    "no routes",
    "resource",
    "framework"
  ],
  "typescript": {
